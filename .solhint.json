{
  "extends": "solhint:recommended",
  "rules": {
    "max-line-length": ["error", 99],
    "separate-by-one-line-in-contract": "error",
    "two-lines-top-level-separator": "error",
    "func-order": "error",
    "reason-string": ["error"],
    "constructor-syntax": "error",
    "compiler-version": "0.5.13"
  }
}
